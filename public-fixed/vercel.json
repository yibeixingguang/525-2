{
  "version": 2,
  "framework": "nextjs",
  "outputDirectory": ".",
  "trailingSlash": true,
  "routes": [
    { "src": "/_next/(.*)", "dest": "/_next/$1" },
    { "src": "/goals", "dest": "/goals.html" },
    { "src": "/logs", "dest": "/logs.html" },
    { "src": "/settings", "dest": "/settings.html" },
    { "src": "/(.*)", "dest": "/$1.html" }
  ]
}
